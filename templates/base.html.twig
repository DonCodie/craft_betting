<!DOCTYPE html>
<html lang="fr">
<head>
    <meta charset="UTF-8">
    <title>{% block title %}Accueil{% endblock %}</title>
    <link rel="icon" href="{{ asset('build/favicon/favicon.png') }}" type="image/x-icon"/>
    {% block stylesheets %}{{ encore_entry_link_tags('app') }}{% endblock %}
</head>
<body>
{% block header %}
    <header class="header">
        <div class="header__banner">
            <div class="header__banner--img-div">
                <img src="{{ asset('build/banner/banner.png') }}" alt="banner" class="header__banner--img"/>
            </div>
        </div>
        {#        <div class="header__menu">#}
        {#            MENU#}
